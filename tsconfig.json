{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["frontaind/*"]
    }
  },
  "include": ["frontaind/**/*.ts", "frontaind/**/*.d.ts", "frontaind/**/*.tsx", "frontaind/**/*.vue"],
  "references": [{ "path": "./tsconfig.node.json" }]
}

{
  "name": "OpenAssistant Falcon 7B with knowledge",
  "aifileversion": 1,
  "chain": {
    "_type": "stuff_documents_chain",
    "callback_manager": null,
    "callbacks": null,
    "document_prompt": {
      "_type": "prompt",
      "input_variables": [
        "page_content"
      ],
      "output_parser": null,
      "partial_variables": {},
      "template": "{page_content}",
      "template_format": "f-string",
      "validate_template": true
    },
    "document_separator": "\n\n",
    "document_variable_name": "summaries",
    "input_key": "input_knowledge",
    "llm_chain": {
      "_type": "llm_chain",
      "callback_manager": null,
      "callbacks": null,
      "llm": {
        "_type": "huggingface_textgen_inference",
        "inference_server_url": "your-server-url",
        "max_new_tokens": 400,
        "repetition_penalty": 1.03,
        "stream": true,
        "temperature": 0.01,
        "top_k": 10,
        "top_p": 0.95,
        "typical_p": 0.95
      },
      "memory": null,
      "output_key": "text",
      "prompt": {
        "_type": "prompt",
        "input_variables": [
          "summaries",
          "input_text",
          "input_history"
        ],
        "output_parser": null,
        "partial_variables": {},
        "template": "<|system|>{summaries}<|endoftext|>{input_history}<|prompter|>{input_text}<|endoftext|><|assistant|>",
        "template_format": "f-string",
        "validate_template": true
      },
      "verbose": false
    },
    "memory": null,
    "output_key": "output_text",
    "verbose": false
  }
}

{
  "name": "Your Model",
  "aifileversion": 1,
  "chain": {
    "memory": null,
    "verbose": false,
    "prompt": {
      "input_variables": [
        "input_text",
        "input_history"
      ],
      "output_parser": null,
      "partial_variables": {},
      "template": "{input_history}\nHuman: {input_text}\nAI:",
      "template_format": "f-string",
      "validate_template": true,
      "_type": "prompt"
    },
    "llm": {
      "inference_server_url": "http://localhost:8010/",
      "max_new_tokens": 512,
      "top_k": 10,
      "top_p": 0.95,
      "typical_p": 0.95,
      "temperature": 0.01,
      "repetition_penalty": 1.03,
      "streaming": true,
      "_type": "huggingface_textgen_inference"
    },
    "output_key": "output_text",
    "_type": "llm_chain"
  }
}

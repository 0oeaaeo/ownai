{
  "name": "LightGPT @Aviary",
  "aifileversion": 1,
  "chain": {
    "memory": null,
    "verbose": false,
    "prompt": {
      "input_variables": [
        "input_text",
        "input_history"
      ],
      "output_parser": null,
      "partial_variables": {},
      "template": "{input_history}\nHuman: {input_text}\nAI:",
      "template_format": "f-string",
      "validate_template": true,
      "_type": "prompt"
    },
    "llm": {
      "aviary_url": "http://example.com/",
      "model": "amazon/LightGPT",
      "_type": "aviary"
    },
    "output_key": "output_text",
    "_type": "llm_chain"
  }
}
